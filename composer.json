{
  "name": "wavevision/links",
  "description": "Link helpers and wrappers for Nette application.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wavevision s.r.o.",
      "email": "info@wavevision.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wavevision\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\LinksExamples\\": "examples/LinksExamples/",
      "Wavevision\\LinksTests\\": "tests/LinksTests/"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "require": {
    "php": ">=7.4",
    "nette/application": "^3.0",
    "wavevision/props-control": "^5.0",
    "wavevision/utils": "^2.13"
  },
  "require-dev": {
    "nette/bootstrap": "^3.0",
    "php-coveralls/php-coveralls": "^2.2",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-nette": "^0.12.9",
    "phpstan/phpstan-phpunit": "^0.12.16",
    "wavevision/coding-standard": "^5.0",
    "wavevision/di-service-annotation": "^4.0",
    "wavevision/nette-tests": "^3.1"
  }
}
